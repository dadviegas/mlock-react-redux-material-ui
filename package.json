    "presets": [
      "env",
      "stage-0"
    ]
  },
  "devDependencies": {
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "css-loader": "^0.28.7",
    "favicons-webpack-plugin": "0.0.7",
    "mblock-react": "^1.1.3",
    "mblock-webpack": "^1.1.3",
    "node-sass": "^4.7.2"
  },
